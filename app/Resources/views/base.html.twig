<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>{% block title %}Administration - {% endblock %}</title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />

    {% block stylesheets %}



        <!-- ================== BEGIN BASE CSS STYLE ================== -->
        <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/jquery-ui/themes/base/minified/jquery-ui.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/css/animate.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/css/style.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/css/style-responsive.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/css/theme/red.css') }}" rel="stylesheet" id="theme" />
        <!-- ================== END BASE CSS STYLE ================== -->

        <!-- ================== BEGIN PAGE LEVEL STYLE ================== -->
        <link href="{{ asset('default/assets/plugins/jquery-jvectormap/jquery-jvectormap.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/bootstrap-datepicker/css/bootstrap-datepicker.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/gritter/css/jquery.gritter.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/DataTables/media/css/dataTables.bootstrap.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/plugins/DataTables/extensions/Responsive/css/responsive.bootstrap.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('default/assets/css/manual.css') }}" rel="stylesheet" />
        <!-- ================== END PAGE LEVEL STYLE ================== -->

        <!-- ================== BEGIN BASE JS ================== -->
        <script src="{{ asset('default/assets/plugins/pace/pace.min.js') }}"></script>
        <!-- ================== END BASE JS ================== -->

    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

</head>
<body>
<!-- begin #page-loader -->
<div id="page-loader" class="fade in"><span class="spinner"></span></div>
<!-- end #page-loader -->

<!-- begin #page-container -->
<div id="page-container" class="fade page-sidebar-fixed page-header-fixed">

    {% block header %}
        <!-- begin #header -->
        <div id="header" class="header navbar navbar-default navbar-fixed-top">
            <!-- begin container-fluid -->
            <div class="container-fluid">
                <!-- begin mobile sidebar expand / collapse button -->
                <div class="navbar-header">
                    <a href="{{ url('homepage') }}" class="navbar-brand"><span class="navbar-logo"></span> ARTEFACTS</a>
                    <button type="button" class="navbar-toggle" data-click="sidebar-toggled">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <!-- end mobile sidebar expand / collapse button -->

                <!-- begin header navigation right -->
                <ul class="nav navbar-nav navbar-right">


                </ul>
                <!-- end header navigation right -->
            </div>
            <!-- end container-fluid -->
        </div>
        <!-- end #header -->
    {% endblock %}

    {% block nav %}

        <!-- begin #sidebar -->
        <div id="sidebar" class="sidebar">
            <!-- begin sidebar scrollbar -->
            <div data-scrollbar="true" data-height="100%">
                <!-- begin sidebar user -->

                <!-- end sidebar user -->
                <!-- begin sidebar nav -->
                <!-- USER -->
                <ul class="nav">
                    <li class="nav-header">Navigation</li>

                    <li class="has-sub">
                        <a href="{{ url('homepage') }}">
                            <i class="fa fa-users"></i>
                            <span>Accueil</span>
                        </a>
                    </li>

                    <li class="has-sub">
                        <a href="{{ url('organisme') }}">
                            <i class="fa fa-list"></i>
                            <span>Organisme</span>
                        </a>
                    </li>
                    <li class="has-sub">
                        <a href="{{ url('editeur') }}">
                            <i class="fa fa-list"></i>
                            <span>Editeur</span>
                        </a>
                    </li>
                    <li class="has-sub">
                        <a href="{{ url('collection') }}">
                            <i class="fa fa-list"></i>
                            <span>Collection</span>
                        </a>
                    </li>
                    <!-- begin sidebar minify button -->
                    {#<li><a href="javascript:;" class="sidebar-minify-btn" data-click="sidebar-minify"><i class="fa fa-angle-double-left"></i></a></li>#}
                    <!-- end sidebar minify button -->
                </ul>
                <!-- end sidebar nav -->
            </div>
            <!-- end sidebar scrollbar -->
        </div>
        <div class="sidebar-bg"></div>
        <!-- end #sidebar -->
        in scroll to top btn -->
        <a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
        <!-- end scroll to top btn -->
    {%  endblock %}

    <!-- begin #content -->
    <div id="content" class="content">
        <!-- Zone de naviguation text -->
        <ol class="breadcrumb pull-right">
            <li><a href="{{ url('homepage') }}">Accueil</a></li>
            <li class="active"> {% block pageName %}{% endblock pageName %}</li>
        </ol>
        <!-- end breadcrumb -->
        <!-- begin page-header -->
        <h1 class="page-header">{% block pageTitle %}{% endblock pageTitle %} <small>{% block pageSmallName %}{% endblock pageSmallName %}</small></h1>
        <!-- end page-header -->

        {% block content %}
        {% endblock %}
    </div>
</div>

{% block javascripts %}
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="{{ asset('default/assets/plugins/jquery/jquery-1.9.1.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/jquery/jquery-migrate-1.1.0.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/jquery-ui/ui/minified/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/bootstrap/js/bootstrap.min.js') }}"></script>
    <!--[if lt IE 9]>
    <script src="{{ asset('default/assets/crossbrowserjs/html5shiv.js') }}"></script>
    <script src="{{ asset('default/assets/crossbrowserjs/respond.min.js') }}"></script>
    <script src="{{ asset('default/assets/crossbrowserjs/excanvas.min.js') }}"></script>
    <![endif]-->
    <script src="{{ asset('default/assets/plugins/slimscroll/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/jquery-cookie/jquery.cookie.js') }}"></script>
    <!-- ================== END BASE JS ================== -->

    <!-- ================== BEGIN PAGE LEVEL JS ================== -->

    <script src="{{ asset('default/assets/plugins/gritter/js/jquery.gritter.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/flot/jquery.flot.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/flot/jquery.flot.time.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/flot/jquery.flot.resize.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/flot/jquery.flot.pie.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/sparkline/jquery.sparkline.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/jquery-jvectormap/jquery-jvectormap.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/jquery-jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js') }}"></script>
    <script src="{{ asset('default/assets/js/dashboard.min.js') }}"></script>
    <script src="{{ asset('default/assets/js/apps.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/DataTables/media/js/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/DataTables/media/js/dataTables.bootstrap.min.js') }}"></script>
    <script src="{{ asset('default/assets/plugins/DataTables/extensions/Responsive/js/dataTables.responsive.min.js') }}"></script>
    <script src="{{ asset('default/assets/js/table-manage-default.demo.min.js') }}"></script>
    <!-- ================== END PAGE LEVEL JS ================== -->


{% endblock %}

{% block init  %}

{%  endblock init %}

</body>
</html>
