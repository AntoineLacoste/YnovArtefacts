<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AuteurBibliographie" table="auteur_bibliographie">
    <indexes>
      <index name="FK_auteur_bibliographie_personne" columns="id_auteur"/>
      <index name="FK_auteur_bibliographie_bibliographie_v2" columns="id_bibliographie"/>
    </indexes>
    <id name="idAuteurBibliographie" type="integer" column="id_auteur_bibliographie">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idBibliographie" target-entity="BibliographieV2" fetch="LAZY">
      <join-columns>
        <join-column name="id_bibliographie" referenced-column-name="id_bibliographie"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAuteur" target-entity="Personne" fetch="LAZY">
      <join-columns>
        <join-column name="id_auteur" referenced-column-name="id_personne"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
