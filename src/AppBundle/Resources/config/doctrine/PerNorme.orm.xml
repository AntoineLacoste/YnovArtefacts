<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\PerNorme" table="per_norme">
    <indexes>
      <index name="FK_per_norme_periodique" columns="id_periodique"/>
      <index name="FK_per_norme_norme" columns="id_norme"/>
    </indexes>
    <id name="idPerNorme" type="integer" column="id_per_norme">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="124" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idNorme" target-entity="Norme" fetch="LAZY">
      <join-columns>
        <join-column name="id_norme" referenced-column-name="id_norme"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPeriodique" target-entity="Periodique" fetch="LAZY">
      <join-columns>
        <join-column name="id_periodique" referenced-column-name="id_periodique"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
