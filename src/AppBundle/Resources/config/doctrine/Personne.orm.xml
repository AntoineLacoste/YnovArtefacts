<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Personne" table="personne">
    <indexes>
      <index name="FK_personne_personne" columns="id_directeur"/>
      <index name="FK_personne_nationalite" columns="id_nationalite"/>
      <index name="FK_personne_organisme" columns="id_organisme"/>
    </indexes>
    <id name="idPersonne" type="integer" column="id_personne">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateVie" type="string" column="date_vie" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="note" type="string" column="note" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idNationalite" target-entity="Nationalite" fetch="LAZY">
      <join-columns>
        <join-column name="id_nationalite" referenced-column-name="id_nationalite"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idOrganisme" target-entity="Organisme" fetch="LAZY">
      <join-columns>
        <join-column name="id_organisme" referenced-column-name="id_organisme"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDirecteur" target-entity="Personne" fetch="LAZY">
      <join-columns>
        <join-column name="id_directeur" referenced-column-name="id_personne"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
